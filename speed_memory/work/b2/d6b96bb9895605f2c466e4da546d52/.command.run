    ln -s /home/carisdak/deepimpute_paper_scripts/speed_memory/work/44/f27f5e99e3ea51df9f78240865df7b/vmstats_SAVER_100_2.txt vmstats_SAVER_100_2.txt
    ln -s /home/carisdak/deepimpute_paper_scripts/speed_memory/work/49/db3593f8727410352b2c1cf5b276b6/time_SAVER_500_1.txt time_SAVER_500_1.txt
    ln -s /home/carisdak/deepimpute_paper_scripts/speed_memory/work/49/db3593f8727410352b2c1cf5b276b6/vmstats_SAVER_500_1.txt vmstats_SAVER_500_1.txt
