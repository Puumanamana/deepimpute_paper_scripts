    ln -s /home/carisdak/deepimpute_paper_scripts/speed_memory/work/63/878edad78d6283694422207c830cd9/time_SAVER_100_1.txt time_SAVER_100_1.txt
    ln -s /home/carisdak/deepimpute_paper_scripts/speed_memory/work/63/878edad78d6283694422207c830cd9/vmstats_SAVER_100_1.txt vmstats_SAVER_100_1.txt
